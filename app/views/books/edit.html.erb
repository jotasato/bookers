<h1>Editing Book</h1>
<!-- form_for部分 -->

<%= form_for(@book) do |f| %>
    <h4>Title</h4>
    <%= f.text_field :title %>
    <h4>Body</h4>
    <%= f.text_field :body %>

    <p><%= f.submit 'Update Book' %></p>
<% end %>


<p>
 <%= link_to "Show", book_path %>
|
 <%= link_to "Back", back_path %>
</p>

